{
  "extends": ["react-app", "prettier"],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "arrowParens": "avoid",
        "semi": false,
        "trailingComma": "none",
        "singleQuote": true,
        "jsxSingleQuote": false,
        "array-callback-return": "off"
      }
    ]
  }
}
